<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.normalrecyclerview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NormalRecyclerView">
        <activity android:name=".swipe.SwipeSelectionActivity"></activity>
        <activity android:name=".multiselection.MultipleSelectionActivity" />
        <activity android:name=".cardviewy.CardViewActivity" />
        <activity android:name=".singles.SingleSelectionRV" />
        <activity android:name=".normalrecycler.NormalRecyclerView" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>